{#
/**
* @file
* Default theme implementation to display a block.
*
* @see template_preprocess_block()
*
* @ingroup themeable
*/
#}
{% set classes = [
    'clear-both',
] %}
<div{{ attributes.addClass(classes) }}>
<strong>{{ data.cdata|date('h:i a', data.ldata['timezone'])  }}</strong><br>
{{ data.cdata|date('l, d F Y', data.ldata['timezone'])  }}<br>
Time in {{ data.ldata['city'] }}, {{ data.ldata['citycode'] }},  {{ data.ldata['country'] }}

</div>
